{
  "name": "@chip-in/dadget",
  "version": "0.0.3",
  "description": "chip-in Dadget",
  "keywords": [
    "chip-in"
  ],
  "bugs": {
    "url": "https://github.com/chip-in/dadget/issues"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./src/index.ts",
  "browser": {
    "./lib/db/SubsetDb.js": "./lib/db/SubsetDbOnBrowser.js",
    "mongodb": false
  },
  "scripts": {
    "prepare": "npm run clean && npm run build",
