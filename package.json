    "test": "mocha"
  },
  "dependencies": {
    "@chip-in/resource-node": "*",
    "async-lock": "^1.0.0",
    "mongodb": "^2.2.31",
    "object-hash": "1.1.8",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/async-lock": "0.0.20",
    "@types/mongodb": "^2.2.11",
    "@types/node": "^8.0.15",
    "@types/object-hash": "0.5.29",
    "@types/uuid": "^3.4.2",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "mocha": "^4.0.1",
    "chai": "^4.1.2",
    "typedoc": "^0.9.0",
    "typescript": "^2.4.2"
  },
  "publishConfig": {
    "access": "public"
  }
